{
  "files.exclude": {
    "amplify/.config": true,
    "amplify/**/*-parameters.json": true,
    "amplify/**/amplify.state": true,
    "amplify/**/transform.conf.json": true,
    "amplify/#current-cloud-backend": true,
    "amplify/backend/amplify-meta.json": true,
    "amplify/backend/awscloudformation": true
  },
  "json.schemas": [
    {
      "fileMatch": ["/paths.json"],
      "url": "./.vscode/paths.schema.json"
    },
    {
      "fileMatch": ["/path-schema.json"],
      "url": "./.vscode/path-schema.schema.json"
    },
    {
      "fileMatch": ["/response-schema.json"],
      "url": "./.vscode/response-schema.schema.json"
    }
  ]
}
